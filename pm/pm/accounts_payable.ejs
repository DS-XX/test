                              <span class="top_button" style="background-color:#00ADAD;" id='fee_add'>Search</span>
                            </div>
                        </div>
                        <table>
                            <thead>
                                <tr id='t_head'>
                                  <th>ID</th>
                                    <th>PAY TO</th>
                                    <th>DUE DATE</th>
                                    <th>TYPE</th>
                                    <th>PAYMENT
                                      METHOD</th>
                                    <th>TOTAL
                                      AMOUNT</th>
                                    <th>SHORTAGE</th>
                                    <th>INVOICE</th>
                                    <th>STATUS</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <th>PM12345</th>
                                    <th>PM12345</th>
                                    <th>PM12345</th>
                                    <th>PM12345</th>
                                    <th>PM12345</th>
                                    <th>PM12345</th>
                                    <th>PM12345</th>
                                    <th style="width: auto;"><div class="row">
                                        <img src="/images/pc/pm/message.svg" style="width:20px;height:20px;margin-right:5px"/>
