 </script>
 <script type="text/javascript" src='/pm/js/background_change.js'></script>
 <!-- 弹窗显示 -->
<script type="text/javascript" >
  window.onload=function(){
  insert_bank();
  change_background('cheque','banking');
  }
 document.getElementById('add_new').onclick=function(e){
   console.log(e);
  modal('add_new','my_modal','modal_out'); //第一个按钮id，第二个弹窗id，第三个遮罩id
  change_background('per_cor','select_1');
 }
</script>

  <script type="text/javascript" src="/pm/js/calendar.min.js"></script>
